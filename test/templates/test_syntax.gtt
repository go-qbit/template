[% IMPORT(
    "bytes",
    "github.com/go-qbit/template/test/templates/extwrapper"
) %]

[% TEMPLATE TestExprSyntax(i int, b bool, s string, t []TestType, ptr *TestType, buf *bytes.Buffer) USE WRAPPER extwrapper.Wrapper() %]
    [% IF i > 0 OR i < 100 AND !b || i <= 200 && i >= 150 || !(i == 0 && !b) %]
        [% len(s) %]
        [% +%]   spaces  [%+ %]
        [% %]   rspaces  [%+ %]
        [% +%]   lspaces  [% %]
    [% END %]

    [% FOR i := 0; i < 10; i++ %]
        [% i %]
    [% END %]

    [% PROCESS extwrapper.ExtTemplate() %]

    [% s[10] | html %]
    [% t[0].StructField.F1["test"] %]

    [% 5 + 10 * 15 / 20 % 4 %]

    [% FOR i,v IN t %]
        [% i %]: [%+ v %]
    [% END %]

    [% buf.Next(10) %]

    [% a := i %]
    [% a = i %]
    [% a %]

    [% PROCESS test1("test") %]

    "Quoted text"
    With new line
[% END %]

[% TEMPLATE test1(s string) %]
    [% s | html | html %]
[% END %]

[% WRAPPER wrapper1() %]
    [% CONTENT %]
[% END %]

[% WRAPPER wrapper2() USE WRAPPER wrapper1() %]
    [% CONTENT %]
[% END %]